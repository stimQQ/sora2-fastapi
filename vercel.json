{
  "version": 2,
  "regions": ["sin1"],
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "250mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "VERCEL": "1",
    "PYTHON_VERSION": "3.12"
  }
}
